{
  "manifest_version": 3,
  "name": "Prompt Manager",
  "version": "1.0.0",
  "description": "Store and organize LLM prompts with local storage",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Prompts&Notes"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}